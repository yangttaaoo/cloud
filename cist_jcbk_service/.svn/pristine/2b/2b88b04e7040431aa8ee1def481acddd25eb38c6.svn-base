package com.cist.warningmanagement.bayonet.model;

public class Bayonet {
	private String azdz;//安装地址
    private String bkjl;//布控距离
    private String dept_code;//预警单位
    private String dept_name;
    private String enable;//是否启用
    private String enable_name;
    private String kkbh;//卡扣编号
    private String kkmc;//卡口名称
	private Integer latitude;//纬度
	private Integer longitude;//经度
    private Integer pk;// 主键
    private String qc;//圈层
    private String road_name;
    private String device_name;
    public String getDevice_name() {
		return device_name;
	}
	public void setDevice_name(String device_name) {
		this.device_name = device_name;
	}
	public String getRoad_name() {
		return road_name;
	}
	public void setRoad_name(String road_name) {
		this.road_name = road_name;
	}
	private String qy1;//区域
    private String sbfx;//设备方向
    private String sfyjts;//是否预警推送 1，推送2，不推送
	private String sfyjts_name;
	private String ssbm;//所属部门
    public String getAzdz() {
		return azdz;
	}
    public String getBkjl() {
		return bkjl;
	}
	
	public String getDept_code() {
		return dept_code;
	}
	public String getDept_name() {
		return dept_name;
	}
	public String getEnable() {
		if(enable!=null){
		return "1".equals(enable)?"启用":"禁用";}
		return enable;
	}
	public String getEnable_name() {
		if(enable!=null){
			enable_name=this.enable;
		}
		return enable_name;
	}
	public String getKkbh() {
		return kkbh;
	}
	public String getKkmc() {
		return kkmc;
	}
	public Integer getLatitude() {
		return latitude;
	}
	public Integer getLongitude() {
		return longitude;
	}
	public Integer getPk() {
		return pk;
	}
	public String getQc() {
		return qc;
	}
	public String getQy1() {
		return qy1;
	}
	public String getSbfx() {
		return sbfx;
	}
	public String getSfyjts() {
		return sfyjts;
	}
	public String getSfyjts_name() {
		if(sfyjts!=null){
			return "1".equals(sfyjts)?"推送":"不推送";
		}
		return sfyjts_name;
	}
	public String getSsbm() {
		return ssbm;
	}
	public void setAzdz(String azdz) {
		this.azdz = azdz;
	}
	public void setBkjl(String bkjl) {
		this.bkjl = bkjl;
	}
	public void setDept_code(String dept_code) {
		this.dept_code = dept_code;
	}
	public void setDept_name(String dept_name) {
		this.dept_name = dept_name;
	}
	public void setEnable(String enable) {
		this.enable = enable;
	}
	public void setEnable_name(String enable_name) {
		this.enable_name = enable_name;
	}
	public void setKkbh(String kkbh) {
		this.kkbh = kkbh;
	}
	public void setKkmc(String kkmc) {
		this.kkmc = kkmc;
	}
	public void setLatitude(Integer latitude) {
		this.latitude = latitude;
	}
	public void setLongitude(Integer longitude) {
		this.longitude = longitude;
	}
	public void setPk(Integer pk) {
		this.pk = pk;
	}
	public void setQc(String qc) {
		this.qc = qc;
	}
	public void setQy1(String qy1) {
		this.qy1 = qy1;
	}
	public void setSbfx(String sbfx) {
		this.sbfx = sbfx;
	}
	public void setSfyjts(String sfyjts) {
		this.sfyjts = sfyjts;
	}
	public void setSfyjts_name(String sfyjts_name) {
		this.sfyjts_name = sfyjts_name;
	}
	public void setSsbm(String ssbm) {
		this.ssbm = ssbm;
	}
    
}
