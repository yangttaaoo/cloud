package com.cist.businessconfig.bayonetmanage.model;
/**
 * 布控卡口拦截单位
 * @author Hy
 *
 */
public class Jcbk_bayonet_units {
	private String pk;//pk
	private String kkbh;//卡口编号
	private String bkkk;//布控卡口PK（关联JCBK_CONTROL_BAYONET表中PK字段）
	private String bmdm;//部门代码
	public String getPk() {
		return pk;
	}
	public void setPk(String pk) {
		this.pk = pk;
	}
	public String getKkbh() {
		return kkbh;
	}
	public void setKkbh(String kkbh) {
		this.kkbh = kkbh;
	}
	public String getBkkk() {
		return bkkk;
	}
	public void setBkkk(String bkkk) {
		this.bkkk = bkkk;
	}
	public String getBmdm() {
		return bmdm;
	}
	public void setBmdm(String bmdm) {
		this.bmdm = bmdm;
	}
	
}
