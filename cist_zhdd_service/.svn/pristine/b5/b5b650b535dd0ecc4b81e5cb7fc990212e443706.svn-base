package com.cist.tqrw.service.impl;

import java.util.HashMap;
import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.cist.frame.page.PageInfo;
import com.cist.tqrw.mapper.TqrwMapper;
import com.cist.tqrw.model.MissionGrade;
import com.cist.tqrw.model.Police;
import com.cist.tqrw.service.TqrwService;
@Service
public class TqrwServiceImpl implements TqrwService{
	@Autowired
	private TqrwMapper mapper;

	@Override
	public Object selectAllpageList(HashMap<String, Object> map, PageInfo pageInfo) {
		return mapper.selectAll(map);
	}

	@Override
	public Integer selectCount() {
		return mapper.selectCount();
	}

	@Override
	public List<MissionGrade> selectGrades() {
		return mapper.selectGrades();
	}

	@Override
	public List<Police> selectPolice() {
		return mapper.selectPolice();
	}

	@Override
	public List<Police> selectPoliceCar() {
		return mapper.selectPoliceCar();
	}

}
