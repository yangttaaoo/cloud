package com.cist.tqrw.model;

import java.io.Serializable;
import java.util.Date;

import com.fasterxml.jackson.annotation.JsonFormat;
/**
 * 特勤任务 
 * @author Administrator
 *
 */
public class SpecialMission implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 5299621226449460804L;
	private Integer pk_id;
	private Integer fk_dept_id;//部门ID
	private Integer fk_create_founder_id;//创建人ID
	@JsonFormat(pattern="yyyy-MM-dd HH:mm:ss", timezone="GTM+8")
	private Date create_time;//创建时间
	private String mission_name;//任务名称
	private Integer fk_mission_grade_id;//任务等级
	@JsonFormat(pattern="yyyy-MM-dd HH:mm:ss", timezone="GTM+8")
	private Date mission_start_time;//任务开始时间
	@JsonFormat(pattern="yyyy-MM-dd HH:mm:ss", timezone="GTM+8")
	private Date mission_end_time;//任务结束时间
	private String mission_describe;//任务描述
	private String sypi_name;//创建人姓名
	private String dept_name;//部门名称
	private String grade_name;//等级名称

	public Integer getPk_id() {
		return pk_id;
	}

	public void setPk_id(Integer pk_id) {
		this.pk_id = pk_id;
	}

	public Integer getFk_dept_id() {
		return fk_dept_id;
	}

	public void setFk_dept_id(Integer fk_dept_id) {
		this.fk_dept_id = fk_dept_id;
	}

	public Integer getFk_create_founder_id() {
		return fk_create_founder_id;
	}

	public void setFk_create_founder_id(Integer fk_create_founder_id) {
		this.fk_create_founder_id = fk_create_founder_id;
	}

	public Date getCreate_time() {
		return create_time;
	}

	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}

	public String getMission_name() {
		return mission_name;
	}

	public void setMission_name(String mission_name) {
		this.mission_name = mission_name;
	}

	public Integer getFk_mission_grade_id() {
		return fk_mission_grade_id;
	}

	public void setFk_mission_grade_id(Integer fk_mission_grade_id) {
		this.fk_mission_grade_id = fk_mission_grade_id;
	}

	public Date getMission_start_time() {
		return mission_start_time;
	}

	public void setMission_start_time(Date mission_start_time) {
		this.mission_start_time = mission_start_time;
	}

	public Date getMission_end_time() {
		return mission_end_time;
	}

	public void setMission_end_time(Date mission_end_time) {
		this.mission_end_time = mission_end_time;
	}

	public String getMission_describe() {
		return mission_describe;
	}

	public void setMission_describe(String mission_describe) {
		this.mission_describe = mission_describe;
	}

	public String getSypi_name() {
		return sypi_name;
	}

	public void setSypi_name(String sypi_name) {
		this.sypi_name = sypi_name;
	}

	public String getDept_name() {
		return dept_name;
	}

	public void setDept_name(String dept_name) {
		this.dept_name = dept_name;
	}

	public String getGrade_name() {
		return grade_name;
	}

	public void setGrade_name(String grade_name) {
		this.grade_name = grade_name;
	}
	
}
