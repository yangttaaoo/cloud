package com.cist.patrolplan.model;

public class SYS_CONFIG {
	private long syci_pk;//ID
	private String syci_type;//参数分类
	private String syci_key;//关键值
	private String syci_name;//参数名称
	private String syci_value;//参数值	
	private String syci_des;//参数说明
	private Character syci_html_type;//参数值显示形式 1-文本框 2-下拉框3-单选框4-复选框 
	private String syci_html;//参数值HTML内容，如<option value="10">10秒</option>	
	private String syci_flag;//是否显示 1-显示 2-不显示 3-显示只读	
	public long getSyci_pk() {
		return syci_pk;
	}
	public void setSyci_pk(long syci_pk) {
		this.syci_pk = syci_pk;
	}
	public String getSyci_type() {
		return syci_type;
	}
	public void setSyci_type(String syci_type) {
		this.syci_type = syci_type;
	}
	public String getSyci_key() {
		return syci_key;
	}
	public void setSyci_key(String syci_key) {
		this.syci_key = syci_key;
	}
	public String getSyci_name() {
		return syci_name;
	}
	public void setSyci_name(String syci_name) {
		this.syci_name = syci_name;
	}
	public String getSyci_value() {
		return syci_value;
	}
	public void setSyci_value(String syci_value) {
		this.syci_value = syci_value;
	}
	public String getSyci_des() {
		return syci_des;
	}
	public void setSyci_des(String syci_des) {
		this.syci_des = syci_des;
	}
	public Character getSyci_html_type() {
		return syci_html_type;
	}
	public void setSyci_html_type(Character syci_html_type) {
		this.syci_html_type = syci_html_type;
	}
	public String getSyci_html() {
		return syci_html;
	}
	public void setSyci_html(String syci_html) {
		this.syci_html = syci_html;
	}
	public String getSyci_flag() {
		return syci_flag;
	}
	public void setSyci_flag(String syci_flag) {
		this.syci_flag = syci_flag;
	}
	
	
	
	

}
