package com.cist.alarm.config.model;

public class AlarmConfig {
	private Integer pk_id;// id
	private String temperature; // 温度阀值（低，高）
	private String voltage;// 电压阀值（低，高）
	private Integer broken_net;// 断网阀值（x秒）
	private Integer power_off;// 断电阀值（x秒）

	public Integer getPk_id() {
		return pk_id;
	}

	public void setPk_id(Integer pk_id) {
		this.pk_id = pk_id;
	}

	public String getTemperature() {
		return temperature;
	}

	public void setTemperature(String temperature) {
		this.temperature = temperature;
	}

	public String getVoltage() {
		return voltage;
	}

	public void setVoltage(String voltage) {
		this.voltage = voltage;
	}

	public Integer getBroken_net() {
		return broken_net;
	}

	public void setBroken_net(Integer broken_net) {
		this.broken_net = broken_net;
	}

	public Integer getPower_off() {
		return power_off;
	}

	public void setPower_off(Integer power_off) {
		this.power_off = power_off;
	}

}
