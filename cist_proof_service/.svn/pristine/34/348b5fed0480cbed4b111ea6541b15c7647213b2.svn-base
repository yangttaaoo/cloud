package com.cist.evidencemanage.model;

import java.math.BigDecimal;
import java.util.Date;

public class ZjjhEvidenceDownloadInfo {
    private BigDecimal pk_id;//ID

    private BigDecimal fk_evidence_id;//证据信息ID

    private BigDecimal fk_use_unit_id;//使用单位ID

    private BigDecimal fk_download_unit_id;//下载单位ID

    private BigDecimal fk_download_person_id;//下载人ID

    private Date download_time;//下载时间

    private String download_reason;//下载证据原因

	public BigDecimal getPk_id() {
		return pk_id;
	}

	public void setPk_id(BigDecimal pk_id) {
		this.pk_id = pk_id;
	}

	public BigDecimal getFk_evidence_id() {
		return fk_evidence_id;
	}

	public void setFk_evidence_id(BigDecimal fk_evidence_id) {
		this.fk_evidence_id = fk_evidence_id;
	}

	public BigDecimal getFk_use_unit_id() {
		return fk_use_unit_id;
	}

	public void setFk_use_unit_id(BigDecimal fk_use_unit_id) {
		this.fk_use_unit_id = fk_use_unit_id;
	}

	public BigDecimal getFk_download_unit_id() {
		return fk_download_unit_id;
	}

	public void setFk_download_unit_id(BigDecimal fk_download_unit_id) {
		this.fk_download_unit_id = fk_download_unit_id;
	}

	public BigDecimal getFk_download_person_id() {
		return fk_download_person_id;
	}

	public void setFk_download_person_id(BigDecimal fk_download_person_id) {
		this.fk_download_person_id = fk_download_person_id;
	}

	public Date getDownload_time() {
		return download_time;
	}

	public void setDownload_time(Date download_time) {
		this.download_time = download_time;
	}

	public String getDownload_reason() {
		return download_reason;
	}

	public void setDownload_reason(String download_reason) {
		this.download_reason = download_reason;
	}

   
}