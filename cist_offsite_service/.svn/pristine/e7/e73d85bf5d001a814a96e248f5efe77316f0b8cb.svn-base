		  (
		  TOOL_PK,
		  TOOL_USER,
		  TOOL_IP,
		  TOOL_TIME,
		  TOOL_DEPT_CODE,
		  TOOL_TYPE,
		  TOOL_RESULT,
		  TOOL_SIGN,
		  TOOL_WFXH,
		  TOOL_SRC,
		  TOOL_DEST
		 )
		  values(
		 cistsys.SEQ_IMGNAME_PK.NEXTVAL,
		  #{tool_user},
		  #{tool_ip},
		  sysdate,
		  #{tool_dept_code},
		  #{tool_type},
		  #{tool_result},
		  #{tool_sign},
		  #{tool_wfxh},
		  #{tool_src,jdbcType=CLOB},
		  #{tool_dest,jdbcType=CLOB}
		  )
  </insert>
	
	
