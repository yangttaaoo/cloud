		   </if>
		   <if test="wfxw!=null and wfxw!=''">
		   tboc_name=#{wfxw} 
		   </if>
  		 </where>   
	</select>
	
	<select id="treeSb" resultType="com.cist.recheck.model.Vw_dev_info">
