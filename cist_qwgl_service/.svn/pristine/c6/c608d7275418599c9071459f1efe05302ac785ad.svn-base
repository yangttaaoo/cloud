package com.cist.feedback.model;

import java.math.BigDecimal;
import java.util.Date;

public class QwAppraiseTopic {

	private BigDecimal fk_user_id;//考核单位id
    
    private BigDecimal lmpk;//栏目pk
    
    private String topic_name;//栏目名称

	public BigDecimal getFk_user_id() {
		return fk_user_id;
	}

	public void setFk_user_id(BigDecimal fk_user_id) {
		this.fk_user_id = fk_user_id;
	}

	public BigDecimal getLmpk() {
		return lmpk;
	}

	public void setLmpk(BigDecimal lmpk) {
		this.lmpk = lmpk;
	}

	public String getTopic_name() {
		return topic_name;
	}

	public void setTopic_name(String topic_name) {
		this.topic_name = topic_name;
	}


}